generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Polygon {
  Id String @id // polygon_id P005_000002

  TreesLikeCount   Int
  BBQLikeCount     Int
  BikesLikeCount   Int
  BooksLikeCount   Int
  FlowersLikeCount Int
}
