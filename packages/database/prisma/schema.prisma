generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Polygon {
  Id String @id // polygon_id P005_000002

  TreesLikeCount         Int
  FlowersLikeCount       Int
  DrinkFountainLikeCount Int
  PublicToiletLikeCount  Int
}
